# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Skaters' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Skaters
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FacebookCore'  
  pod 'FacebookLogin'  
  pod 'NCMB', :git => 'https://github.com/takuyama29/ncmb_ios-2.3.3'
  target 'SkatersTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SkatersUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
